{
    "manifest_version": 3,
    "key": "",
    "name": "MitXSS",
    "version": "1.0",
    "description": "Prevent against malicious URLs and Cross-site scripting attacks",
    "permissions": ["tabs", "activeTab","declarativeNetRequest", "webNavigation", "storage"],
    "action": {
      "default_popup": "src/popup/popup.html",
      "default_icon": "src/images/xss.png"
    },
    "background": {
      "service_worker": "dist/background/background.js"
    },
    "icons": {
      "16": "src/images/xss.png",
      "48": "src/images/xss.png",
      "128": "src/images/xss.png"
    },
    "web_accessible_resources": [
      {
          "resources": ["popup.html"],
          "matches": ["<all_urls>"]
      }
    ],
    "host_permissions": [
      "http://127.0.0.1:8000/urlapp/check/"
    ],
    "declarative_net_request": {
      "rules_resources": [
        {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
        

  }
    
  }
  